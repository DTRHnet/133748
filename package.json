{
  "name": "133748",
  "version": "0.2.0-alpha-04",
  "description": "A rock-solid 'feels-native' command-line tool for Ultimate Guitar tabs. (Playing on 'leetab' / 'elitetab')",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "133748": "bin/133748"
  },
  "scripts": {
    "build": "babel src --out-dir dist --extensions \".js,.jsx\"",
    "build:web": "npm run build && npm run build:functions",
    "build:functions": "babel netlify/functions --out-dir netlify/functions/dist --extensions \".js\"",
    "start": "node dist/index.js",
    "start:web": "netlify dev",
    "search": "npm run build && node dist/index.js search",
    "grab": "npm run build && node dist/index.js grab",
    "setup": "node setup.js",
    "run": "node dist/index.js",
    "run:win": "run.bat",
    "run:unix": "./run.sh",
    "test": "vitest",
    "test:integration": "vitest run --dir test/integration",
    "test:unit": "vitest run --dir test/unit",
    "test:watch": "vitest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "precommit": "lint-staged",
    "prepare": "husky",
    "clean": "rimraf dist netlify/functions/dist",
    "rebuild": "npm run clean && npm run build",
    "dev": "npm run build && npm run start",
    "dev:web": "npm run build:web && npm run start:web",
    "deploy": "netlify deploy --prod",
    "deploy:preview": "netlify deploy"
  },
  "keywords": [
    "cli",
    "ultimate-guitar",
    "tabs",
    "fzf",
    "guitar-pro",
    "node",
    "133748",
    "leetab",
    "elitetab"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.3",
    "@babel/node": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@netlify/plugin-lighthouse": "^4.0.0",
    "esbuild": "^0.25.5",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "netlify-cli": "^17.0.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.4",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "ink": "^5.2.1",
    "open": "^10.1.2",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "puppeteer": "^24.9.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^18.3.1"
  }
}
