{
  "name": "133748",
  "version": "0.2.0-alpha-04",
  "description": "A rock-solid 'feels-native' command-line tool for Ultimate Guitar tabs. (Playing on 'leetab' / 'elitetab')",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "133748": "bin/133748"
  },
  "scripts": {
    "build": "cross-env babel src --out-dir dist --extensions \".js,.jsx\"",
    "postbuild": "cross-env node scripts/copy-static.js",
    "start": "cross-env node dist/index.js",
    "web": "npm run build && cross-env node dist/web/server.js",
    "search": "npm run build && cross-env node dist/index.js search",
    "test": "cross-env vitest",
    "test:integration": "cross-env vitest run --dir test/integration",
    "test:unit": "cross-env vitest run --dir test/unit",
    "lint": "cross-env eslint .",
    "lint:fix": "cross-env eslint . --fix",
    "format": "cross-env prettier --write .",
    "precommit": "cross-env lint-staged",
    "prepare": "cross-env husky"
  },
  "keywords": [
    "cli",
    "ultimate-guitar",
    "tabs",
    "fzf",
    "guitar-pro",
    "node",
    "133748",
    "leetab",
    "elitetab"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.3",
    "@babel/node": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/plugin-transform-class-properties": "^7.23.4",
    "@babel/plugin-transform-object-rest-spread": "^7.23.4",
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/runtime": "^7.23.6",
    "core-js": "^3.35.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.5",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "express": "^4.18.2",
    "ink": "^5.2.1",
    "multer": "^1.4.5-lts.1",
    "open": "^10.1.2",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "puppeteer": "^24.9.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^18.3.1",
    "socket.io": "^4.7.4"
  }
}
