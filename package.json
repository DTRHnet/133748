{
  "name": "133748",
  "version": "0.2.0-alpha-04",
  "description": "A rock-solid 'feels-native' command-line tool for Ultimate Guitar tabs. (Playing on 'leetab' / 'elitetab')",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "bin": {
    "133748": "bin/133748"
  },
  "scripts": {
    "build": "node_modules\\.bin\\babel.cmd src --out-dir dist --extensions \".js,.jsx\"",
    "build:web": "npm run build && npm run build:functions && npm run build:react",
    "build:functions": "node scripts/build-functions.js",
    "build:public": "node scripts/build-public.js",
    "build:static": "node scripts/build-static.js",
    "build:netlify": "npm run build:functions && npm run build:static",
    "build:react": "vite build",
    "dev:react": "vite",
    "start:react": "node src/web/server.js",
    "serve": "node src/web/server.js",
    "start": "node dist/index.js",
    "start:web": "netlify dev --command \"npm run start:react\"",
    "search": "npm run build && node dist/index.js search",
    "grab": "npm run build && node dist/index.js grab",
    "setup": "node setup.js",
    "run": "node dist/index.js",
    "run:win": "run.bat",
    "run:unix": "./run.sh",
    "test": "vitest",
    "test:integration": "vitest run --dir test/integration",
    "test:unit": "vitest run --dir test/unit",
    "test:watch": "vitest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rimraf dist netlify/functions/dist",
    "rebuild": "npm run clean && npm run build",
    "dev": "npm run build && npm run start",
    "dev:web": "npm run build:web && npm run start:web",
    "deploy": "netlify deploy --prod",
    "deploy:preview": "netlify deploy"
  },
  "keywords": [
    "cli",
    "ultimate-guitar",
    "tabs",
    "fzf",
    "guitar-pro",
    "node",
    "133748",
    "leetab",
    "elitetab"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.3",
    "@babel/node": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "esbuild": "^0.25.5",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "netlify-cli": "^18.0.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.4",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/plugin-transform-class-properties": "^7.27.0",
    "@babel/plugin-transform-object-rest-spread": "^7.27.0",
    "@babel/plugin-transform-runtime": "^7.27.0",
    "@babel/runtime": "^7.27.0",
    "@vitejs/plugin-react": "^4.3.1",
    "babel-core": "^6.26.3",
    "cheerio": "^1.0.0",
    "core-js": "^3.39.0",
    "cross-env": "^7.0.3",
    "express": "^5.1.0",
    "ink": "^5.2.1",
    "is-ci": "^3.0.1",
    "node": "^22.21.0",
    "open": "^10.1.2",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "puppeteer": "^23.0.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite": "^5.4.21"
  }
}
